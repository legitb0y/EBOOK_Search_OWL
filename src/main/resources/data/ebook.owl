<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/ebook#"
     xml:base="http://www.example.org/ebook"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
     
    <!-- Khai báo ontology -->
    <owl:Ontology rdf:about="http://www.example.org/ebook"/>

    <!-- Định nghĩa các lớp -->
    <owl:Class rdf:about="#Ebook"/>
    <owl:Class rdf:about="#Author"/>
    <owl:Class rdf:about="#Genre"/>
    <owl:Class rdf:about="#Publisher"/>
    <owl:Class rdf:about="#Reader"/>
    <owl:Class rdf:about="#Review"/>

    <!-- Định nghĩa các thuộc tính -->
    <owl:ObjectProperty rdf:about="#writtenBy">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="#Author"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#publishedBy">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="#Publisher"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#belongsToGenre">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="#Genre"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reviewedBy">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="#Review"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#readBy">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="#Reader"/>
    </owl:ObjectProperty>

    <rdf:Property rdf:about="#publicationYear">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="xsd:gYear"/>
    </rdf:Property>

    <rdf:Property rdf:about="#isbn">
        <rdfs:domain rdf:resource="#Ebook"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </rdf:Property>

    <!-- Định nghĩa các cá thể -->
    <owl:NamedIndividual rdf:about="#Ebook1">
        <rdf:type rdf:resource="#Ebook"/>
        <isbn>978-3-16-148410-0</isbn>
        <publicationYear>2023</publicationYear>
        <writtenBy rdf:resource="#Author1"/>
        <publishedBy rdf:resource="#Publisher1"/>
        <belongsToGenre rdf:resource="#Genre1"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Author1">
        <rdf:type rdf:resource="#Author"/>
        <rdfs:label>Jane Doe</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Publisher1">
        <rdf:type rdf:resource="#Publisher"/>
        <rdfs:label>TechBooks Publishing</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Genre1">
        <rdf:type rdf:resource="#Genre"/>
        <rdfs:label>Technology</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Reader1">
        <rdf:type rdf:resource="#Reader"/>
        <rdfs:label>John Smith</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Review1">
        <rdf:type rdf:resource="#Review"/>
        <rdfs:label>"An excellent resource for learning!"</rdfs:label>
        <reviewedBy rdf:resource="#Reader1"/>
    </owl:NamedIndividual>

</rdf:RDF>

